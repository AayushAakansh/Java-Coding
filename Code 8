import java.util.*;
public class Boards9
{
    public void main()
    { Scanner s = new Scanner(System.in);
        System.out.println("Enter a Number: ");
        int num= s.nextInt();
        if(num%2==0)
        System.out.println("The Number is Even");
        else
        System.out.println("The Number is Odd");
        
    
}
}
